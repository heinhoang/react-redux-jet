{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "defaultSeverity": "warning",
  "linterOptions": {
      "exclude": [
          "node_modules/**/*.ts",
          "src/serviceWorker.ts"
      ]
  },
  "rules": {
      "indent": [true, "spaces", 2],
      "max-line-length": [true, 120]
  }
}
