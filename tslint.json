{
    "extends": ["tslint:recommended", "tslint-config-standard", "tslint-config-prettier"],
    "defaultSeverity": "warning",
    "linterOptions": {
      "exclude": [
        "node_modules/**/*.ts"
      ]
    },
    "rules": {
      "indent": [true, "spaces", 2],
      "max-line-length": [true, 120]
    }
  }